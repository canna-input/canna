#include "../../Canna.conf"

      CANNAROOT = ../..
           SRCS = forcpp.c

#if defined(__EMX__) || defined(__CYGWIN32__)
AllTarget(forcpp.exe)
#else
AllTarget(forcpp)
#endif
SingleProgramTarget(forcpp,forcpp.o,,)
InstallProgram(forcpp,$(cannaBinDir))
DependTarget()
LintTarget()

