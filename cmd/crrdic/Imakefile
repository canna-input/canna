#include "../../Canna.conf"

      CANNAROOT = ../..
       INCLUDES = -I$(CANNAROOT)/include
           SRCS = crrdic.c

#if defined(__EMX__) || defined(__CYGWIN32__)
AllTarget(crrdic.exe)
#else
AllTarget(crrdic)
#endif
SingleProgramTarget(crrdic,crrdic.o,,)
InstallProgram(crrdic,$(cannaBinDir))
DependTarget()
LintTarget()
