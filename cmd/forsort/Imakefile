#include "../../Canna.conf"

      CANNAROOT = ../..
           SRCS = forsort.c

#if defined(__EMX__) || defined(__CYGWIN32__)
AllTarget(forsort.exe)
#else
AllTarget(forsort)
#endif
SingleProgramTarget(forsort,forsort.o,,)
InstallProgram(forsort,$(cannaBinDir))
DependTarget()
LintTarget()
