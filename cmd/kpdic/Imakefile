#include "../../Canna.conf"

    CANNAROOT = ../..
     INCLUDES = -I$(CANNAROOT)/include
         SRCS = kpdic.c

#if defined(__EMX__) || defined(__CYGWIN32__)
AllTarget(kpdic.exe)
#else
AllTarget(kpdic)
#endif
SingleProgramTarget(kpdic,kpdic.o,,)
InstallProgram(kpdic,$(cannaBinDir))
DependTarget()
LintTarget()

