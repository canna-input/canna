#include "../../Canna.conf"

      CANNAROOT = ../..
       INCLUDES = -I$(CANNAROOT)/lib/RK -I$(CANNAROOT)/include
           SRCS = mergewd.c

#if defined(__EMX__) || defined(__CYGWIN32__)
AllTarget(mergeword.exe)
#else
AllTarget(mergeword)
#endif
NormalProgramTarget(mergeword,mergewd.o,,,)
InstallProgram(mergeword,$(cannaBinDir))
DependTarget()
LintTarget()
