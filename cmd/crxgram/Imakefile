#include "../../Canna.conf"

      CANNAROOT = ../..
       INCLUDES = -I$(CANNAROOT)/include
LOCAL_LIBRARIES = $(CANNAROOT)/lib/RK/libRK.a
           SRCS = crxgram.c

#if defined(__EMX__) || defined(__CYGWIN32__)
AllTarget(crxgram.exe)
#else
AllTarget(crxgram)
#endif
NormalProgramTarget(crxgram,crxgram.o,,,)
InstallProgram(crxgram,$(cannaBinDir))
DependTarget()
LintTarget()
