#include "../../Canna.conf"

      CANNAROOT = ../..
       INCLUDES = -I$(CANNAROOT)/include
           SRCS = ctow.c

#if defined(__EMX__) || defined(__CYGWIN32__)
AllTarget(ctow.exe)
#else
AllTarget(ctow)
#endif
SingleProgramTarget(ctow,ctow.o,,)
InstallProgram(ctow,$(cannaBinDir))
InstallManPage(ctow,$(cannaManDir))
DependTarget()
LintTarget()
