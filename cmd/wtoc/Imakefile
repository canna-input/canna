#include "../../Canna.conf"

      CANNAROOT = ../..
           SRCS = wtoc.c

#if defined(__EMX__) || defined(__CYGWIN32__)
AllTarget(wtoc.exe)
#else
AllTarget(wtoc)
#endif
SingleProgramTarget(wtoc,wtoc.o,,)
InstallProgram(wtoc,$(cannaBinDir))
InstallManPage(wtoc,$(cannaManDir))
DependTarget()
LintTarget()
