/* $Id: Imakefile,v 1.2 2002/10/20 04:10:28 aida_s Exp $ */
#include "../../Canna.conf"

      CANNAROOT = ../..
       PROGRAMS = mkbindic
           SRCS = mkbindic.cpp

        DEFINES = -I$(CANNAROOT)/include $(SCRIPTS_DEFINES) -DCPP=$(CPP) \
                  $(useObsoleteFileName)
#ifdef __CYGWIN32__
DEFINES := $(DEFINES) -Ui386
#endif

AllTarget($(PROGRAMS))

CppScriptTarget(mkbindic,mkbindic.cpp,$(DEFINES),/**/)
InstallNamedProg(mkbindic,mkbindic,$(cannaBinDir))
InstallManPage(mkbindic,$(cannaManDir))

clean::
	$(RM) $(PROGRAMS)

depend::
